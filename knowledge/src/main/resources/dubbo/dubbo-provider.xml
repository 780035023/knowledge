<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd 
	http://code.alibabatech.com/schema/dubbo 
	http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<bean id="demoService" class="com.ixinnuo.financial.knowledge.dubbo.provider.DemoServiceImpl"/>

	<!-- 提供方应用信息，用于计算依赖关系 -->
	<dubbo:application name="${dubbo.application.name1}" />

	<!-- 注册中心暴露服务地址,可选的 -->
	<!-- <dubbo:registry address="multicast://224.5.6.7:1234" /> -->

	<!-- <dubbo:registry protocol="zookeeper" address="10.170.219.98:2181,10.173.55.173:2181" 
		/> -->
	<dubbo:registry protocol="${dubbo.registry.protocol}" 
		address="${dubbo.registry.address}" />

	<!-- 暴露服务 
	dubbo://192.168.8.55:20880/com.ixinnuo.financial.knowledge.dubbo.DemoService?anyhost=true&application=example-provider&dubbo=2.5.5&generic=false&interface=com.ixinnuo.financial.knowledge.dubbo.DemoService&methods=sayHello&pid=27228&retries=0&side=provider&timeout=6000&timestamp=1506495093891
	-->
	<dubbo:protocol name="${dubbo.protocol.name}" port="${dubbo.protocol.port}" />
	<!-- 服务接口及实现类 -->
	<dubbo:service interface="com.ixinnuo.financial.knowledge.dubbo.DemoService"
		ref="demoService" retries="0" timeout="6000" />
</beans>